<Config>

    <Class Modifier="internal" Name="SearchHandshakePacket">
        <Properties>
            <Array Name="SearchProtocolVersions" Id="0" Limit="32">
                <Element>
                    <Varint Type="SearchProtocolVersion" />
                </Element>
            </Array>
        </Properties>
    </Class>

    <Class Modifier="internal" Name="ExchangeHandshakePacket">
        <Properties>
            <Array Name="ExchangeProtocolVersions" Id="0" Limit="32">
                <Element>
                    <Varint Type="ExchangeProtocolVersion" />
                </Element>
            </Array>
        </Properties>
    </Class>

    <Class Modifier="internal" Name="SearchProfilePacket">
        <Properties>
            <Bytes Name="Id" Id="0" Limit="32" />
            <Message Name="Location" Id="1" Type="Location" />
        </Properties>
    </Class>

    <Class Modifier="internal" Name="ExchangeProfilePacket">
        <Properties>
            <Bytes Name="Id" Id="0" Limit="32" />
            <Message Name="Location" Id="1" Type="Location" />
        </Properties>
    </Class>

    <Class Modifier="internal" Name="LocationsPublishPacket">
        <Properties>
            <Array Name="Locations" Id="0" Limit="256">
                <Element>
                    <Message Type="Location" />
                </Element>
            </Array>
        </Properties>
    </Class>

    <Class Modifier="internal" Name="LocationsRequestPacket">
        <Properties>
            <Array Name="Metadatas" Id="0" Limit="256">
                <Element>
                    <Message Type="Metadata" />
                </Element>
            </Array>
        </Properties>
    </Class>

    <Class Modifier="internal" Name="LocationsResultPacket">
        <Properties>
            <Message Name="Metadata" Id="0" Type="Metadata" />
            <Array Name="Locations" Id="1" Limit="256">
                <Element>
                    <Message Type="Location" />
                </Element>
            </Array>
        </Properties>
    </Class>

    <Class Modifier="internal" Name="MetadatasRequestPacket">
        <Properties>
            <Array Name="BroadcastMetadataSignatures" Id="0" Limit="1024 * 8">
                <Element>
                    <Message Type="Signature" />
                </Element>
            </Array>
            <Array Name="UnicastMetadataSignatures" Id="1" Limit="1024 * 8">
                <Element>
                    <Message Type="Signature" />
                </Element>
            </Array>
            <Array Name="MulticastMetadataTags" Id="2" Limit="1024 * 8">
                <Element>
                    <Message Type="Tag" />
                </Element>
            </Array>
        </Properties>
    </Class>

    <Class Modifier="internal" Name="MetadatasResultPacket">
        <Properties>
            <Array Name="BroadcastMetadatas" Id="0" Limit="1024 * 8">
                <Element>
                    <Message Type="BroadcastMetadata" />
                </Element>
            </Array>
            <Array Name="UnicastMetadatas" Id="1" Limit="1024 * 8">
                <Element>
                    <Message Type="UnicastMetadata" />
                </Element>
            </Array>
            <Array Name="MulticastMetadatas" Id="2" Limit="1024 * 8">
                <Element>
                    <Message Type="MulticastMetadata" />
                </Element>
            </Array>
        </Properties>
    </Class>

    <Class Modifier="internal" Name="MetadatasLookupRequest">
        <Properties>
            <Array Name="Metadatas" Id="0" Limit="1024 * 8">
                <Element>
                    <Message Type="Metadata" />
                </Element>
            </Array>
        </Properties>
    </Class>

    <Class Modifier="internal" Name="MetadatasLookupResult">
        <Properties>
            <Array Name="Metadatas" Id="0" Limit="1024 * 8">
                <Element>
                    <Message Type="Metadata" />
                </Element>
            </Array>
        </Properties>
    </Class>

    <Class Modifier="internal" Name="BlocksRequestPacket">
        <Properties>
            <Array Name="Hashes" Id="0" Limit="1024 * 8">
                <Element>
                    <Message Type="Hash" />
                </Element>
            </Array>
        </Properties>
    </Class>

    <Class Modifier="internal" Name="BlockResultPacket">
        <Properties>
            <Message Name="Hash" Id="0" Type="Hash" />
            <Bytes Name="Value" Id="1" IsRecyclable="true" Limit="1024 * 1024 * 4"/>
        </Properties>
    </Class>

</Config>
