    "configurations": [
        {
            "name": "daemon (Win)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build (Omnius.Axis.Daemon)",
            "program": "${workspaceFolder}/tmp/bin/win-vscode/Omnius.Axis.Daemon/Debug/net6.0/Omnius.Axis.Daemon.dll",
            "args": [
                "-s",
                "./storage/daemon",
                "-l",
                "tcp(ip4(127.0.0.1),43200)",
                "-v",
                "true"
